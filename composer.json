{
    "name": "mostafaznv/nova-file-artisan",
    "description": "File Upload Tool for Laravel Nova",
    "keywords": [
        "file",
        "upload",
        "image",
        "video",
        "hls",
        "stream",
        "media",
        "file manager",
        "storage",
        "s3",
        "sftp",
        "file system",
        "resize",
        "crop",
        "ffmpeg",
        "m3u8",
        "laravel",
        "mostafaznv"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "laravel/framework": "^10.4.1|^11.0",
        "laravel/nova": "^4.27",
        "mostafaznv/larupload": "^1.0.5|^2.0.0",
        "nova-kit/nova-packages-tool": "^v1.13.3"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\NovaFileArtisan\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaznv\\NovaFileArtisan\\NovaFileArtisanServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
